@model Userccount
@{
    ViewData["Title"] = "Register";
}



<!-- contact section -->
<section class="contact_section layout_padding">
    <div class="container">
        <div class="heading_container">
            <h2>
                Register
            </h2>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form_container contact-form">
                    <form asp-action="Register" method="post" enctype="multipart/form-data">
                        <div class="form-row">
                            <div class="col-lg-6">
                                <div>
                                    <input name="Fullname" id="Fullname" asp-for="Fullname" type="text" placeholder="Enter Your Name" />
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div>
                                    <input name="Phonenumber" id="Phonenumber" asp-for="Phonenumber" type="text" placeholder="Enter Your Phone Number" />
                                </div>
                            </div>
                        </div>
                        <div>
                            <input name="Email" id="Email" asp-for="Email" type="email" placeholder="Enter Your Email" />
                        </div>

                        <div>
                            <input name="ImageFile" id="ImageFile" asp-for="ImageFile" type="file" placeholder="Choose Your Image" />
                        </div>
                        <div>
                            <input name="Password" id="Password" asp-for="Password" type="password" placeholder="Enter Your Password" />
                        </div>

                        <p style="color:red">@ViewBag.Error</p>

                        
                        <div class="btn_box">
                            <button>
                                Register
                            </button>
                        </div>
                    </form>
                </div>
            </div>
            @*<div class="col-md-6">
                <div class="map_container">
                    <div class="map">
                        <div id="googleMap"></div>
                    </div>
                </div>
            </div>*@
        </div>
    </div>
</section>
<!-- end contact section -->
